/* CSS Document */
@font-face { font-family: Cinzel; src: url('/font/Cinzel-Regular.ttf'); }

h1{color:#a8a288; font-size:40px; text-transform:uppercase; margin-bottom:17px; line-height:1.2em; font-family: Cinzel;}
h2{color:#a8a288; font-size:30px; text-transform:uppercase; margin-bottom:17px; line-height:1.2em; font-family: Cinzel;}
h3{color:#a8a288; font-size:20px; text-transform:uppercase; margin-bottom:17px; line-height:1.2em; font-family: Cinzel;}
h4{color:#b8b4a5; font-size:1em; margin-bottom:9px; font-family: Cinzel;}

.title1{color:#a8a288; font-size:1.6em; font-family: Cinzel;}
.title2{color:#a8a288; font-size:1.3em; font-family: Cinzel;}

.message1{color:#000; font-size:1.6em;}

heading{color:#000; font-size:1.6em;}

.link-1 { display:inline-block; line-height:29px; background:#bdc074; padding:0 17px 0 22px; color:#fff; font-weight:bold; text-decoration:none; border-radius:25px;	-moz-border-radius:25px; -webkit-border-radius:25px; cursor:pointer; position:relative;}
.link-1:hover { background:#a8a288; text-decoration:none; color:#fff}

.link-2 { display:inline-block; line-height:29px; background:#bdc074; padding:0 17px 0 22px; color:#fff; font-weight:bold; text-decoration:none; border-radius:25px;	-moz-border-radius:25px; -webkit-border-radius:25px; cursor:pointer; position:relative;}
.link-2:hover { background:#a8a288; text-decoration:none; color:#fff}

.subtitle1{color:#000; font-size:.8em;}

